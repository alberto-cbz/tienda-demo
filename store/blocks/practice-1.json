    },
    "flex-layout.col#info-top-col-2" : {
        "children" : [
            "image#infto-top-img2"
        ],
        "props" : { 
            "blockClass" : "info-top-col"
        }
    },
    "flex-layout.col#info-top-col-3" : {
        "children" : [
            "image#infto-top-img3"
        ],
        "props" : {
            "blockClass" : "info-top-col"
        }
    },
    "flex-layout.row#banner-1" : {
